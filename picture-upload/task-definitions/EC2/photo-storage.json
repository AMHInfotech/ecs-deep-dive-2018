{
    "family": "photo-storage",
    "requiresCompatibilities": [
        "EC2"
    ],
    "memory": "512",
    "cpu": "256",
    "taskRoleArn": "arn:aws:iam::123456789012:role/PhotoStorageTaskRole",
    "containerDefinitions": [
        {
            "name": "photo-storage",
            "image": "123456789012.dkr.ecr.us-east-1.amazonaws.com/photo-storage:latest",
            "portMappings": [
                {
                    "containerPort": 3001
                }
            ],
            "command": [
                "npm",
                "run",
                "prod"
            ],
            "environment": [
                {
                    "name": "DEBUG",
                    "value": "*"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "production-service-photo-storage",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "photo-storage"
                }
            }
        }
    ]
}