{
  "family": "photo-storage",
  "requiresCompatibilities": [
      "EC2"
  ],
  "networkMode": "bridge",
  "containerDefinitions": [
      {
          "name": "photo-storage",
          "image": "123456789012.dkr.ecr.us-east-1.amazonaws.com/photo-storage:latest",
          "essential": true,
          "portMappings": [
              {
                  "hostPort": 0,
                  "containerPort": 3001,
                  "protocol": "tcp"
              }
          ],
          "command": [
              "npm",
              "run",
              "prod"
          ],
          "environment": [
              {
                  "name": "DEBUG",
                  "value": "*"
              }
          ],
          "logConfiguration": {
              "logDriver": "awslogs",
              "options": {
                  "awslogs-group": "/ecs/photo-storage",
                  "awslogs-region": "us-east-1",
                  "awslogs-stream-prefix": "ecs"
              }
          }
      }
  ],
  "memory": "512",
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::123456789012:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::123456789012:role/PhotoStorageTaskRole",
  "volumes": [],
  "placementConstraints": []
}