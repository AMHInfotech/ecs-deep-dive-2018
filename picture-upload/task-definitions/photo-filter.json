{
  "family": "photo-filter",
  "requiresCompatibilities": [
    "EC2"
  ],
  "networkMode": "bridge",
  "containerDefinitions": [
    {
      "name": "photo-filter",
      "image": "123456789012.dkr.ecr.us-east-1.amazonaws.com/photo-filter:latest",
      "memoryReservation": "300",
      "cpu": "256",
      "essential": true,
      "portMappings": [
        {
          "hostPort": "0",
          "containerPort": "3002",
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/photo-filter",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "memory": "512",
  "cpu": "256",
  "executionRoleArn": "arn:aws:iam::123456789012:role/ecsTaskExecutionRole",
  "volumes": [],
  "placementConstraints": []
}